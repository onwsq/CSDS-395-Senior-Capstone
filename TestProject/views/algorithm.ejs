<div class="algorithmbody">
    <div class="container">
        <h1>Profile Selection</h1>
        <div class="row" id="rowprofiles">
          <% for(var i = 0 ; i < algorithmlist.length ; i++) { %>
            <label>
              <input type="checkbox" name="" />
              <div class="icon-box">
                <i class="fa fa-heart"></i>
                <span><%=algorithmlist[i]%>
                </span>
              </div>
            </label>
          <% } %>
        </div>
      </div>
    </div>
</div>


</html>
